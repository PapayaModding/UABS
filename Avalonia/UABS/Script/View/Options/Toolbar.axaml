<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="UABS.Options.Toolbar">

    <UserControl.Styles>
        <StyleInclude Source="avares://UABS/Script/Styles/ToolbarStyle.axaml"/>
    </UserControl.Styles>

    <StackPanel Orientation="Horizontal" Background="#FFFFFF" DockPanel.Dock="Top" Spacing="10">
        <Button x:Name="FileButton" Content="File" Classes="toolbarButton" />
        <Button Content="Export" Classes="toolbarButton" />
        <Button Content="Search" Classes="toolbarButton"/>
        <Button Content="Depend" Classes="toolbarButton"/>
        <Button Content="Filter" Classes="toolbarButton"/>
        <Button Content="Memo" Classes="toolbarButton"/>
        <Button Content="Package" Classes="toolbarButton"/>
        <Button Content="Back" Classes="toolbarButton"/>
    </StackPanel>

</UserControl>
